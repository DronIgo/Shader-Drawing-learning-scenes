code = "shader_type canvas_item;

uniform sampler2D screenTex: hint_screen_texture, repeat_enable;
uniform vec2 playerPos;
varying vec2 playerPosScreen;

void vertex() {
	// Called for every vertex the material is visible on.
	
	playerPosScreen = (CANVAS_MATRIX * vec4(playerPos,0.0,1.0)).xy;
	
}

void fragment() {
	// Called for every pixel the material is visible on.
	//vec2 pixSize = SCREEN_PIXEL_SIZE;
	//vec3 scrColLeft = texture(screenTex,SCREEN_UV - vec2(pixSize.x,0.0)).rgb;
	////vec3 scrCol= texture(screenTex,SCREEN_UV).rgb;
	//float grayLeft = dot(scrColLeft,vec3(0.21,0.72,0.07));
	//
	//vec3 scrColRight = texture(screenTex,SCREEN_UV + vec2(pixSize.x,0.0)).rgb;
	////vec3 scrCol= texture(screenTex,SCREEN_UV).rgb;
	//float grayRight = dot(scrColRight,vec3(0.21,0.72,0.07));
	//
	//vec3 scrColUp = texture(screenTex,SCREEN_UV - vec2(0.0,pixSize.y)).rgb;
	////vec3 scrCol= texture(screenTex,SCREEN_UV).rgb;
	//float grayUp = dot(scrColUp,vec3(0.21,0.72,0.07));
	//
	//vec3 scrColDown = texture(screenTex,SCREEN_UV + vec2(0.0,pixSize.y)).rgb;
	////vec3 scrCol= texture(screenTex,SCREEN_UV).rgb;
	//float grayDown = dot(scrColDown,vec3(0.21,0.72,0.07));
	//
	////COLOR = texture(screenTex,vec2(SCREEN_UV.x,SCREEN_UV.y));
	//COLOR = vec4(vec3(max(abs(grayLeft-grayRight),abs(grayUp-grayDown))),1.0);
	
	vec4 fragCoord = FRAGCOORD;
	
	COLOR = texture(screenTex,SCREEN_UV);
	if (length(playerPosScreen - fragCoord.xy) <= 7.0){
		COLOR = vec4(0.0,1.0,0.0,1.0);
	}
}

