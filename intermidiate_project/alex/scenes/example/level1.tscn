code = "shader_type canvas_item;

uniform float pixX : hint_range(1.0, 1152.0, 1.0);
uniform float pixY : hint_range(1.0, 1152.0, 1.0);
uniform vec4 color1 : source_color;
uniform vec4 color2 : source_color;
uniform sampler2D noise : repeat_enable;
uniform sampler2D scr_tex : hint_screen_texture, repeat_enable;
uniform float fire_size : hint_range(0.0, 1.0, 0.01);
uniform float fire_control : hint_range(1.0, 100.0, 1.0);
void vertex() {
	// Called for every vertex the material is visible on.
}

void fragment() {
	float kletka = max(1.0/pixY, 1.0 / 1152.0);
	float j = floor(UV.y/kletka)*kletka;
	float i = floor(UV.x/kletka)*kletka;
	vec2 new_UV= vec2(j + 0.5 * kletka, i + 0.5 * kletka);
	vec2 fire_direction_l = vec2 ((TIME/6.0)* -1.0, (TIME/6.0));
	float noise_val = texture(noise,new_UV - TIME/6.0).x ;
	float noise_val_l = texture(noise,new_UV - fire_direction_l ).x;
	vec4 fireColor_l = mix(color1, color2, noise_val_l);
	noise_val_l = noise_val_l / fire_control + fire_size;
	float noise_val_r = texture(noise,new_UV + TIME/6.0).x;
	vec4 fireColor_r = mix(color1, color2, noise_val_r);
	noise_val_r = noise_val_r / fire_control + fire_size;
	//float noise_valNorm = noise_val_l - 0.5; 
	vec4 tex_color = texture(scr_tex,SCREEN_UV);
	

	if (new_UV.y > 0.5){
		COLOR = mix(tex_color,fireColor_r,clamp(step(noise_val_l,1.0 - new_UV.y ) + step(noise_val_r, new_UV.y), 0.0, 0.9));
	}
	else{
		COLOR = mix(tex_color,fireColor_l,clamp(step(noise_val_l,1.0 - new_UV.y ) + step(noise_val_r, new_UV.y), 0.0, 0.9));
	}
	//COLOR = mix(tex_color,fireColor_r,clamp(step(noise_val_l,1.0 - new_UV.y ) + step(noise_val_r, new_UV.y), 0.0, 1.0));
	//COLOR *= mix(tex_color,fireColor,clamp(step(noise_val_r, new_UV.y - 0.6), 0.0, 1.0));

}

//void light() {
//	// Called for every pixel for every light affecting the CanvasItem.
//	// Uncomment to replace the default light processing function with this one.
//}
"

