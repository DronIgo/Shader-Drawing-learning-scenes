code = "shader_type canvas_item;
instance uniform float die = 0.0;
instance uniform float hit = 0.0;
uniform sampler2D noise: repeat_enable;
uniform vec4 modulate_color:  source_color;

void vertex() {
	if((hit) > 0.5) {
		COLOR= modulate_color;
	}

		//Some code here

}

void fragment() {
	float noise_val = texture(noise, UV + TIME/4.0).x;
	COLOR *= texture(TEXTURE, vec2(UV.x + noise_val / 280.0 , UV.y + noise_val / 80.0));
	float xz = texture(noise, UV + TIME/2.5).x;
	if ((die) > xz) {
		COLOR = vec4(0.0,0.0,0.0,0.0);

	}

	// Called for every pixel the material is visible on.
}


